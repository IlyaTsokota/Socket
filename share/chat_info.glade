<interface>
  <requires lib="gtk+" version="3.20"/>
   <object class="GtkGrid" id="grid_setting_panel">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="hexpand">True</property>
    <property name="vexpand">True</property>
    <child>
        <object class="GtkGrid" id="setting_form">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child>
            <object class="GtkGrid">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <child>
                <object class="GtkGrid">
                <property name="margin_top">40</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <child>
                    <object class="GtkLabel" id="nameLable">
                    <property name="width_request">50</property>
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="halign">end</property>
                    <property name="hexpand">True</property>
                    <property name="label" translatable="yes">chat name</property>
                    <property name="justify">right</property>
                    <style>
                        <class name="label"/>
                    </style>
                    </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">0</property>
                    </packing>
                </child>
                <child>
                    <object class="GtkEntry" id="nameInput">
                    <property name="width_request">100</property>
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="halign">start</property>
                    <property name="valign">start</property>
                    <property name="hexpand">True</property>
                    <property name="input_purpose">name</property>
                    <property name="margin_right">40</property>
                    <style>
                        <class name="input_edit"/>
                    </style>
                    </object>
                    <packing>
                    <property name="left_attach">1</property>
                    <property name="top_attach">0</property>
                    </packing>
                </child>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">1</property>
                </packing>
            </child>
            <child>
                <object class="GtkButton" id="apply_btn">
                <property name="label" translatable="yes">Apply changes</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
                <property name="margin_top">30</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <style>
                 <class name="color_normal"/>

                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">2</property>
                </packing>
            </child>
             <child>
                <object class="GtkButton" id="add_participants">
                <property name="label" translatable="yes"> Add participants</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <style>
                 <class name="color_normal"/>

                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">3</property>
                </packing>
            </child>
             <child>
                <object class="GtkButton" id="delete_participants">
                <property name="label" translatable="yes">Delete participants</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">end</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <style>
                 <class name="color_normal"/>

                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">4</property>
                </packing>
            </child>
            <child>
                <object class="GtkButton" id="delete_chat">
                <property name="label" translatable="yes">Delete chat</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">end</property>
                <property name="vexpand">True</property>
                <property name="margin_bottom">50</property>
                <property name="hexpand">True</property>

                <style>
                    <class name="color_red"/>
                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">5</property>
                </packing>
            </child>
            
            <child>
                <object class="GtkGrid">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                    <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="orientation">vertical</property>
                    <property name="baseline_position">bottom</property>
                    <child>
                        <object class="GtkImage" id="curr_img">
                         <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="halign">center</property>
                        <property name="margin_left">20</property>
                        <property name="margin_right">20</property>
                        <property name="margin_top">25</property>
                        <property name="hexpand">True</property>
                        <property name="pixbuf">resources/img/round.png</property>
                        </object>
                        <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">0</property>
                        </packing>
                    </child>
                    <child>
                        <object class="GtkFileChooserButton" id="select_img">
                        <property name="height_request">20</property>
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="halign">center</property>
                        <property name="valign">start</property>
                        <property name="margin_top">5</property>
                        <property name="hexpand">True</property>
                        <property name="vexpand">True</property>
                        <property name="title" translatable="yes"/>
                        </object>
                        <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">1</property>
                        </packing>
                    </child>
                    </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">1</property>
                    </packing>
                </child>
                <child>
                    <object class="GtkLabel" id="login_edit">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="valign">center</property>
                    <property name="margin_top">15</property>
                    <property name="hexpand">True</property>
                    <property name="vexpand">True</property>
                    <property name="label" translatable="yes">iromanovic</property>
                    <style>
                        <class name="login_edit"/>
                    </style>
                    </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">0</property>
                    </packing>
                </child>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">0</property>
                </packing>
            </child>
            </object>
            <packing>
            <property name="left_attach">0</property>
            <property name="top_attach">0</property>
            </packing>
        </child>
        <style>
            <class name="setting_form"/>
        </style>
        </object>
        <packing>
        <property name="left_attach">0</property>
        <property name="top_attach">0</property>
        </packing>
    </child>
    <style>
        <class name="message-panel"/>
    </style>
    </object>
</interface>