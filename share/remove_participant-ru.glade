<interface>
  <requires lib="gtk+" version="3.20"/>
   <object class="GtkGrid" id="grid_setting_panel">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="hexpand">True</property>
    <property name="vexpand">True</property>
    <child>
        <object class="GtkGrid" id="setting_form">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child>
            <object class="GtkGrid">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <child>
                <object class="GtkScrolledWindow" id="scroll_by_users">
                <property name="height_request">250</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="margin_top">40</property>
                <property name="hexpand">True</property>
                <property name="vexpand">False</property>
                <property name="shadow_type">in</property>
                <child>
                    <object class="GtkViewport">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="hexpand">True</property>
                    <property name="vexpand">True</property>
                    <child>
                        <object class="GtkGrid" id="users_container">
                        <property name="height_request">70</property>
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="valign">start</property>
                        <property name="hexpand">True</property>
                        <property name="vexpand">False</property>
                      
                        <style>
                            <class name="border-none"/>
                        </style>
                        </object>
                    </child>
                    <style>
                        <class name="border-none"/>
                    </style>
                    </object>
                </child>
                <style>
                    <class name="border-none"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">1</property>
                </packing>
            </child>
            <child>
                <object class="GtkButton" id="back">
                <property name="label" translatable="yes">Назад</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">start</property>
                <property name="vexpand">True</property>
                                <property name="margin_bottom">50</property>

                <property name="hexpand">True</property>
                <style>
                    <class name="color_normal"/>
                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">2</property>
                </packing>
            </child>
            
            <child>
                <object class="GtkGrid">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                    <object class="GtkLabel" id="login_edit">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="halign">center</property>
                    <property name="margin_top">25</property>
                    <property name="margin_start">25</property>
                    <property name="hexpand">True</property>
                    <property name="vexpand">True</property>
                    <property name="label" translatable="yes">Участники</property>
                    <style>
                        <class name="login_edit"/>
                    </style>
                    </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">0</property>
                </packing>
                </child>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">0</property>
                </packing>
            </child>
            </object>
            <packing>
            <property name="left_attach">0</property>
            <property name="top_attach">0</property>
            </packing>
        </child>
        <style>
            <class name="setting_form"/>
        </style>
        </object>
        <packing>
        <property name="left_attach">0</property>
        <property name="top_attach">0</property>
        </packing>
    </child>
    <style>
        <class name="message-panel"/>
    </style>
    </object>
</interface>