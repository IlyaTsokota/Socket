<interface>
  <requires lib="gtk+" version="3.20"/>
   <object class="GtkGrid" id="grid_setting_panel">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="hexpand">True</property>
    <property name="vexpand">True</property>
    <child>
        <object class="GtkGrid" id="setting_form">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child>
            <object class="GtkGrid">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            
          <child>
                   <object class="GtkGrid">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="margin_top">50</property>
                    <child>
                    <object class="GtkGrid">
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="hexpand">True</property>
                        <child>
                        <object class="GtkLabel" id="loginLable">
                            <property name="width_request">50</property>
                            <property name="visible">True</property>
                            <property name="can_focus">False</property>
                            <property name="halign">end</property>
                            <property name="margin_right">10</property>
                            <property name="hexpand">True</property>
                            <property name="label" translatable="yes">логин</property>
                            <property name="justify">right</property>
                             <style>
                            <class name="label"/>
                        </style>
                        </object>
                        <packing>
                            <property name="left_attach">0</property>
                            <property name="top_attach">0</property>
                        </packing>
                        </child>
                        <child>
                        <object class="GtkEntry" id="loginInput">
                            <property name="name">login</property>
                            <property name="width_request">100</property>
                            <property name="visible">True</property>
                            <property name="can_focus">True</property>
                            <property name="has_focus">True</property>
                            <property name="halign">start</property>
                            <property name="valign">start</property>
                            <property name="margin_right">70</property>
                            <property name="hexpand">True</property>
                            <property name="input_purpose">name</property>
                            <style>
                                <class name="input_edit"/>
                            </style>
                        </object>
                        <packing>
                            <property name="left_attach">1</property>
                            <property name="top_attach">0</property>
                        </packing>
                        </child>
                    </object>
                    <packing>
                        <property name="left_attach">0</property>
                        <property name="top_attach">0</property>
                    </packing>
                    </child>
                    <child>
                    <object class="GtkLabel" id="fail_login">
                        <property name="name">danger_lable</property>
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="margin_top">10</property>
                          <style>
                                <class name="logout"/>
                            </style>
                    </object>
                    <packing>
                        <property name="left_attach">0</property>
                        <property name="top_attach">1</property>
                    </packing>
                  </child>
                   </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">1</property>
                    </packing>
                </child>
             <child>
                <object class="GtkButton" id="add_participants">
                <property name="label" translatable="yes">Добавить участника</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">end</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <style>
                 <class name="color_normal"/>

                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">2</property>
                </packing>
            </child>
           
            <child>
                <object class="GtkButton" id="back">
                <property name="label" translatable="yes">Назад</property>
                <property name="width_request">420</property>
                <property name="height_request">70</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">start</property>
                <property name="vexpand">True</property>
                <property name="hexpand">True</property>
                <property name="margin_bottom">50</property>

                <style>
                    <class name="color_normal"/>
                    <class name="btn_info"/>
                </style>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">3</property>
                </packing>
            </child>
            
            <child>
                <object class="GtkGrid">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                    <object class="GtkLabel" id="login_edit">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="halign">start</property>
                    <property name="margin_top">25</property>
                    <property name="margin_start">25</property>
                    <property name="hexpand">True</property>
                    <property name="vexpand">True</property>
                    <property name="label" translatable="yes">Добавить участника</property>
                    <style>
                        <class name="login_edit"/>
                    </style>
                    </object>
                    <packing>
                    <property name="left_attach">0</property>
                    <property name="top_attach">0</property>
                </packing>
                </child>
                </object>
                <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">0</property>
                </packing>
            </child>
            </object>
            <packing>
            <property name="left_attach">0</property>
            <property name="top_attach">0</property>
            </packing>
        </child>
        <style>
            <class name="setting_form"/>
        </style>
        </object>
        <packing>
        <property name="left_attach">0</property>
        <property name="top_attach">0</property>
        </packing>
    </child>
    <style>
        <class name="message-panel"/>
    </style>
    </object>
</interface>